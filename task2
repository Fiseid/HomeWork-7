m = {}

stock = {
    "banana": 6,
    "apple": 0,
    "orange": 32,
    "pear": 15
}
prices = {
    "banana": 4,
    "apple": 2,
    "orange": 1.5,
    "pear": 3
}

for i, x in stock.items():
    for y, j in prices.items():
        if i == y:
            b = x * j
            m.setdefault(i, b)
print(m) 